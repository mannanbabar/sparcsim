
clear

gcc -c memory.c -o memory.o -I.
gcc -c loader.c -o loader.o -I.
gcc -c bits.c -o bits.o -I.
gcc -c misc.c -o misc.o -I.
gcc -c decode.c -o decode.o -I.
gcc -c register.c -o register.o -I.
gcc -c execute.c -o execute.o -I.
gcc -c breakpoint.c -o breakpoint.o -I.
gcc -c trap.c -o trap.o -I.
gcc -c simulator.c -o simulator.o -I.

gcc loader.o memory.o decode.o register.o bits.o misc.o execute.o breakpoint.o trap.o simulator.o -o sparcsim -lelf

rm -f *.o
